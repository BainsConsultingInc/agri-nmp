@using Microsoft.AspNetCore.Http;
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="HandheldFriendly" content="true" />
    <meta name="description" content="" />
    <meta name="author" content="Ministry of Environment" />
    <META HTTP-EQUIV="Pragma" CONTENT="no-cache">
    <META HTTP-EQUIV="Expires" CONTENT="-1">


    <title>@ViewBag.Title</title>

    <!-- BOOTSTRAP CORE CSS v3.3.6 -->

    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <link rel="stylesheet" href="~/lib/bootstrap/css/ie10-viewport-bug-workaround.css" />

    <!-- STANDARD CSS -->
    <link type="text/css" href="~/lib/bootstrap/css/main.css?refresh=160106102" rel="stylesheet" />

    <!-- FONTS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" />

    <!-- CUSTOM ENV CSS -->
    <link href="~/lib/bootstrap/css/custom-main.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/css/custom-form.css" rel="stylesheet" />

    <!-- SITE CSS -->
    @*<link href="~/css/mygovbc-bootstrap-theme.min.css" rel="stylesheet" />*@
    <link href="~/css/site.css?refresh=@DateTime.Now.Ticks" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/lib/bootstrap/js/misc.js"></script>
    <script src="~/lib/bootstrap/js/navMenu.js"></script>
    <script src="~/lib/bootstrap/js/ie10-viewport-bug-workaround.js"></script>
    <script src="~/lib/jquery.validate.min.js"></script>
    <script src="~/lib/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/lib/jquery.validate.unobtrusive.min.js"></script>
    <!-- DatePicker CSS -->
    <link href="~/lib/bootstrap/css/bootstrap-datepicker.min.css" rel="stylesheet" />
    <script src="~/lib/bootstrap/js/bootstrap-datepicker.min.js"></script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-110010219-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-110010219-1');
    </script>
</head>
<body>
    <div id="wrap" class="content">

        <!-- HEADER -->
        <div id="header">
            <div id="header-main" class="navbar navbar-default ">

                <div class="container">
                    <div id="header-main-row" class="row">
                        <!-- GOV & ENV TITLE -->
                        <div class="col-sm-3 col-md-2 col-lg-2 header-main-left">
                            <div id="logo">
                                <a href="http://gov.bc.ca"><img src="~/images/gov3_bc_logo.png" alt="Government of B.C." title="Government of B.C." /></a>
                            </div>
                            <div>
                                <span style="padding-left:20px; font-size: 30px;font-weight: 700;color: #fff;top: 15px;position: relative;white-space: nowrap;" role="banner">Nutrient Management Calculator</span>
                            </div>
                            <button type="button" class="navbar-toggle gov-button-custom collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="true" aria-label="Burger Navigation">
                                <img src="~/images/menu-open-mobile.png" />
                            </button>
                        </div>
                        <!-- /GOV & ENV TITLE -->
                        <!-- COLLAPSING NAVBAR -->
                        <div id="navbar" class="collapse navbar-collapse">
                            <ul class="nav navbar-nav">
                                <li><a href='@Url.Action("About", "Home" )' target="_blank">About</a></li>
                                <li><a href='@Url.Action("Help", "Home" )' target="_blank">Help</a></li>
                            </ul>
                        </div>
                        <!-- /COLLAPSING NAVBAR -->

                    </div>
                </div>
                <div class="navigationRibbon">
                    <div class="level2Navigation">
                        <div class="container">
                            <div class="col-sm-3 col-md-2 col-lg-2 header-main-left">
                                <span>
                                    <a href='@Url.Action("About", "Home" )' target="_blank">
                                        About
                                    </a>
                                </span>
                                <span>
                                    <a href='@Url.Action("Help", "Home" )' target="_blank">
                                        Help
                                    </a>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-height"></div>

        <!--WIZARD-->
        <div class="wizard">
            <div class="wrapper-large wizard-list clearfix" style="text-align:right">
                <a href='@Url.Action("Report", "Report" )'><u>Continue later</u></a>
            </div>
            <ul class="wrapper wizard-list clearfix">
                <li id="wizard-home" class="wizard-item ">
                    <a href='@Url.Action("Index", "Home" )'>
                        <span class="wizard-name">Home</span>
                    </a>
                </li>
                <li id="wizard-farm" class="wizard-item">
                    <a id="firstMenu" href='@Url.Action("Farm", "Farm" )'>
                        <span class="wizard-name">Farm Info</span>
                    </a>
                </li>
                <li id="wizard-manure" class="wizard-item">
                    <a id="fourthMenu" href='@Url.Action("ManureGeneratedObtained", "ManureManagement" )'>
                        <span class="wizard-name">Manure/Compost</span>
                    </a>
                </li>
                <li id="wizard-field" class="wizard-item">
                    <a id="secondMenu" href='@Url.Action("Fields", "Fields" )'>
                        <span class="wizard-name">Fields and Soil</span>
                    </a>
                </li>
                <li id="wizard-nutrient" class="wizard-item">
                    <a id="fifthMenu" href='@Url.Action("Calculate", "Nutrients" )'>
                        <span class="wizard-name">Calculate Nutrients</span>
                    </a>
                </li>
                <li id="wizard-report" class="wizard-item">
                    <a id="sixthMenu" href='@Url.Action("Report", "Report" )'>
                        <span class="wizard-name">Reporting</span>
                    </a>
                </li>
            </ul>

        </div>
        <div class="submenu-manure-wizard">
            <ul class="submenu-manure-wrapper submenu-manure-wizard-list clearfix">
                <li id="wizard-submanuregen" class="submenu-manure-wizard-item" style="display:none">
                    <a id="sMenu" href='@Url.Action("ManureGeneratedObtained", "ManureManagement" )'>
                        <span class="wizard-name">Manure generated</span>
                    </a>
                </li>
                <li id="wizard-submanureimported" class="submenu-manure-wizard-item" style="display:none">
                    <a id="eMenu" href='@Url.Action("ManureImported", "ManureManagement" )'>
                        <span class="wizard-name">Manure/Compost imported </span>
                    </a>
                </li>
                <li id="wizard-submanurestorage" class="submenu-manure-wizard-item" style="display:none">
                    <a id="nMenu" href='@Url.Action("ManureStorage", "ManureManagement" )'>
                        <span class="wizard-name">Storage</span>
                    </a>
                </li>
                <li id="wizard-submanunutrient" class="submenu-manure-wizard-item" style="display:none">
                    <a id="tMenu" href='@Url.Action("ManureNutrientAnalysis", "ManureManagement" )'>
                        <span class="wizard-name">Nutrient Analysis</span>
                    </a>
                </li>
                <li id="wizard-subfields" class="submenu-fields-wizard-item" style="display:none">
                    <a id="eMenu" href='@Url.Action("Fields", "Fields" )'>
                        <span class="wizard-name">Field List</span>
                    </a>
                </li>
                <li id="wizard-subsoiltests" class="submenu-fields-wizard-item" style="display:none">
                    <a id="twMenu" href='@Url.Action("SoilTest", "Soil" )'>
                        <span class="wizard-name">Soil Tests</span>
                    </a>
                </li>
            </ul>
        </div>
        <!-- /WIZARD -->
        <!-- /Header -->
        <div>
            @RenderBody()
        </div>
    </div>
    <footer id="footer" style="border-top:none"></footer>
    @RenderSection("Scripts", required: true)
</body>
</html>
